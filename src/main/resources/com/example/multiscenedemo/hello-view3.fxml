<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.multiscenedemo.HelloController3">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label alignment="TOP_CENTER" prefHeight="54.0" prefWidth="379.0" text="Scene 3" wrapText="true">
      <font>
         <Font name="System Bold" size="48.0" />
      </font>
   </Label>
   <HBox alignment="CENTER" prefHeight="55.0" prefWidth="600.0">
      <children>
         <Label prefHeight="74.0" prefWidth="111.0" text="Name:">
            <font>
               <Font size="24.0" />
            </font>
         </Label>
         <TextField fx:id="scene3NameField" prefHeight="37.0" prefWidth="455.0" />
      </children>
   </HBox>
   <HBox alignment="CENTER_LEFT" prefHeight="55.0" prefWidth="600.0">
      <children>
         <Label prefHeight="55.0" prefWidth="175.0" text="  ID Number:">
            <font>
               <Font size="24.0" />
            </font>
         </Label>
         <TextField fx:id="scene3IdField" prefHeight="37.0" prefWidth="345.0" />
      </children>
   </HBox>
    <Button onAction="#onChangeButtonClick" text="Change Scene" />
</VBox>
